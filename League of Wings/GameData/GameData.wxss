/* League of Wings/GameData/GameData.wxss */
.titlebar {
  width: 100%;
  height: 80rpx;
  text-align: center;

  background-color: #ffffff;
  color: #000000;
}

.season-view{
  width:20%;
  float:left;
  line-height:80rpx;
  font-size:35rpx;
  text-align:right;
}

.tab-view{
  width: 20%;
  float:left;
  margin-left: 5%;
  border: 1px solid #a19e9e;
  /* background: #1B2426; */
  height: 50rpx;
  margin-top: 15rpx;
  line-height:50rpx;
  font-size: 30rpx;
  color:#8f97a2;
}

.groupNO{
  width: 10%;
  height: 70rpx;
  background-color: #ffffff;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#000000;
  float: left;
  text-align: center;
}

.groupNOItem{
  width: 100%;
  height: 70rpx;
  background-color: #ffffff;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#000000;
  float: left;
  text-align: center;
  border-top:0.3rpx solid #ffffff;

}

.group{
  width: 90%;
  float: right;
  border-top:0.3rpx solid #ffffff;

}

.groupName{
  width: 40%;
  height: 70rpx;
  background-color: #ffffff;
  font-size: 40rpx;
  line-height: 55rpx;
  color:#000000;
  float: left;
  text-align: center;
}

.groupTitleName{
  width: 45%;
  height: 70rpx;
  background-color: #ffffff;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#000000;
  float: left;
  text-align: center;
}

.grouptitletitle{
  width: 55%;
  height: 70rpx;
  background-color: #ffffff;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#000000;
  float:right;
  
}

.grouptitle{
  width: 60%;
  height: 70rpx;
  background-color: #ffffff;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#000000;
  float:right;
  
}

.grouptitle-view{
  float: left;
  width: auto;
   margin-left: 15%; 
}

.groupicon{
  height: 50rpx;
  width: 50rpx;
  float: left;
}

.NoView{
  overflow: display;
}
.swiper-tab{  
    width: 100%;  
    border-bottom: 2rpx solid #000000;  
    text-align: center;  
    line-height: 80rpx;}  
.swiper-tab-list{  font-size: 30rpx;  
    display: inline-block;  
    width: 33.33%;  
    color: #000000;  
}  
.on{ color: #000000;  
    border-bottom: 5rpx solid #da7c0c;}  
  
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }  
.swiper-box view{  
    text-align: center;  
}  