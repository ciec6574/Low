/* League of Wings/GameData/GameData.wxss */
.titlebar {
  width: 100%;
  height: 80rpx;
  text-align: center;

  background-color: #ffffff;
  color: #000000;
}

.season-view{
  width:20%;
  float:left;
  line-height:80rpx;
  font-size:35rpx;
  text-align:right;
}

.tab-view{
  width: 20%;
  float:left;
  margin-left: 5%;
  border: 1px solid #a19e9e;
  /* background: #1B2426; */
  height: 50rpx;
  margin-top: 15rpx;
  line-height:50rpx;
  font-size: 30rpx;
  color:#8f97a2;
}

.groupNO{
  width: 10%;
  height: 70rpx;
  background-color: #404c5a;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#ffffff;
  float: left;
  text-align: center;
}

.groupNOItem{
  width: 100%;
  height: 70rpx;
  background-color: #404c5a;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#ffffff;
  float: left;
  text-align: center;
  border-top:0.3rpx solid #555555;

}

.group{
  width: 90%;
  float: right;
  border-top:0.3rpx solid #555555;

}

.groupName{
  width: 40%;
  height: 70rpx;
  background-color: #404c5a;
  font-size: 40rpx;
  line-height: 55rpx;
  color:#ffffff;
  float: left;
  text-align: center;
}

.groupTitleName{
  width: 45%;
  height: 70rpx;
  background-color: #404c5a;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#ffffff;
  float: left;
  text-align: center;
}

.grouptitletitle{
  width: 55%;
  height: 70rpx;
  background-color: #404c5a;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#ffffff;
  float:right;
  
}

.grouptitle{
  width: 60%;
  height: 70rpx;
  background-color: #404c5a;
  font-size: 25rpx;
  line-height: 70rpx;
  color:#ffffff;
  float:right;
  
}

.grouptitle-view{
  float: left;
  width: auto;
   margin-left: 15%; 
}

.groupicon{
  height: 55rpx;
  width: 60rpx;
  float: left;
}

.NoView{
  overflow: display;
}
